<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context="scar.apps.scarservice.place">
    <include layout="@layout/product_select_action" android:id="@+id/include2"/>
    <androidx.constraintlayout.widget.ConstraintLayout android:layout_marginTop="?attr/actionBarSize" android:layout_width="0dp"
                                                       android:layout_height="0dp"
                                                       app:layout_constraintHeight_percent="0.55"
                                                       app:layout_constraintWidth_percent="0.7"
                                                       app:layout_constraintEnd_toEndOf="parent"
                                                       app:layout_constraintBottom_toBottomOf="parent"
                                                       app:layout_constraintStart_toStartOf="parent"
                                                       app:layout_constraintTop_toTopOf="parent">
        <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                           android:layout_height="0dp"
                                                           app:layout_constraintHeight_percent="0.68"
                                                           android:id="@+id/constraintLayout8"
                                                           app:layout_constraintTop_toTopOf="parent"
                                                           app:layout_constraintBottom_toTopOf="@+id/constraintLayout9">
            <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                               android:layout_height="0dp"
                                                               app:layout_constraintHeight_percent="0.4"
                                                               android:id="@+id/constraintLayout10"
                                                               app:layout_constraintTop_toTopOf="parent">
                <TextView android:textSize="20dp" android:fontFamily="@font/helvetica_bold" android:background="@drawable/edit_text_backgroud" android:gravity="center_horizontal|center_vertical" android:text="Ряд" app:layout_constraintWidth_percent="0.25" android:layout_width="0dp"
                          android:layout_height="match_parent" android:id="@+id/textView3"
                          app:layout_constraintStart_toStartOf="parent"/>
                <TextView android:layout_width="0dp" app:layout_constraintWidth_percent="0.089"
                          android:layout_height="match_parent"
                          android:id="@+id/textView10"
                          app:layout_constraintStart_toEndOf="@+id/textView3" android:layout_marginStart="8dp"/>
                <TextView android:textSize="20dp" android:fontFamily="@font/helvetica_bold" android:background="@drawable/edit_text_backgroud" android:gravity="center_horizontal|center_vertical" android:text="Стеллаж" app:layout_constraintWidth_percent="0.25" android:layout_width="0dp"
                          android:layout_height="match_parent"
                          android:id="@+id/textView9" android:layout_marginStart="8dp"
                          app:layout_constraintStart_toEndOf="@+id/textView10"/>
                <TextView android:layout_width="0dp" app:layout_constraintWidth_percent="0.089"
                          android:layout_height="match_parent"
                          android:id="@+id/textView8" android:layout_marginStart="8dp"
                          app:layout_constraintStart_toEndOf="@+id/textView9"/>
                <TextView android:textSize="20dp" android:fontFamily="@font/helvetica_bold" android:background="@drawable/edit_text_backgroud" android:gravity="center_horizontal|center_vertical" android:text="Полка" app:layout_constraintWidth_percent="0.25" android:layout_width="0dp"
                          android:layout_height="match_parent"
                          android:id="@+id/textView7" android:layout_marginStart="8dp"
                          app:layout_constraintStart_toEndOf="@+id/textView8"/>
            </androidx.constraintlayout.widget.ConstraintLayout>
            <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                               android:layout_height="0dp"
                                                               app:layout_constraintHeight_percent="0.4"
                                                               app:layout_constraintBottom_toBottomOf="parent"
                                                               android:layout_marginBottom="8dp">
                <EditText android:inputType="textCapCharacters" android:textAlignment="center"
                          android:imeOptions="flagNoExtractUi" android:background="@drawable/edit_text_backgroud"
                          android:layout_width="0dp" app:layout_constraintWidth_percent="0.25" android:layout_height="match_parent"
                          app:layout_constraintStart_toStartOf="parent" android:id="@+id/row"/>
                <TextView android:fontFamily="@font/helvetica_bold" android:textSize="25dp" android:gravity="center_horizontal|center_vertical" app:layout_constraintWidth_percent="0.089" android:text="–" android:layout_width="0dp" android:layout_height="match_parent"
                          android:layout_marginStart="8dp" app:layout_constraintStart_toEndOf="@+id/row"
                          android:id="@+id/textView12"/>
                <EditText android:inputType="number" android:textAlignment="center" android:imeOptions="flagNoExtractUi"
                          android:id="@+id/rack" android:background="@drawable/edit_text_backgroud"
                          android:layout_width="0dp" app:layout_constraintWidth_percent="0.25" android:layout_height="match_parent"
                          app:layout_constraintStart_toEndOf="@+id/textView12" android:layout_marginStart="8dp"/>
                <TextView android:fontFamily="@font/helvetica_bold" android:textSize="25dp" android:gravity="center_horizontal|center_vertical" app:layout_constraintWidth_percent="0.089" android:text="–" android:layout_width="0dp" android:layout_height="match_parent" android:id="@+id/textView11"
                          android:layout_marginStart="8dp" app:layout_constraintStart_toEndOf="@+id/rack"/>

                <EditText android:inputType="number" android:textAlignment="center" android:imeOptions="flagNoExtractUi"
                          android:background="@drawable/edit_text_backgroud"  android:layout_width="0dp" app:layout_constraintWidth_percent="0.25"
                          android:layout_height="match_parent"
                          android:id="@+id/shelf"
                          app:layout_constraintStart_toEndOf="@+id/textView11" android:layout_marginStart="8dp"/>
            </androidx.constraintlayout.widget.ConstraintLayout>

        </androidx.constraintlayout.widget.ConstraintLayout>
        <androidx.constraintlayout.widget.ConstraintLayout android:layout_width="match_parent"
                                                           android:layout_height="0dp"
                                                           app:layout_constraintHeight_percent="0.28"
                                                           app:layout_constraintBottom_toBottomOf="parent"
                                                           android:id="@+id/constraintLayout9"
                                                           app:layout_constraintTop_toBottomOf="@+id/constraintLayout8">
            <Button android:textSize="20dp" android:fontFamily="@font/helvetica_bold" android:textColor="#fff" android:background="#417505"  android:text="ПРИВЯЗАТЬ" android:id="@+id/ok_place" android:layout_width="150dp"
                    android:layout_height="match_parent"
                    app:layout_constraintEnd_toStartOf="@+id/cancel_place" android:layout_marginEnd="8dp"/>
            <Button android:textSize="20dp"  android:background="#f00" android:fontFamily="@font/helvetica_bold" android:textColor="#fff"  android:text="ОТМЕНА" android:id="@+id/cancel_place" android:layout_width="150dp"
                    android:layout_height="match_parent"
                    app:layout_constraintEnd_toEndOf="parent"/>
        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.constraintlayout.widget.ConstraintLayout>
